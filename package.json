{
  "name": "todo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest --config ./jest.config.js --maxWorkers=50%",
    "test:clear-cache": "jest --clearCache",
    "test:detect-open-handles": "jest --clearCache && jest --config ./jest.config.js --maxWorkers=50% --detectOpenHandles",
    "test:watch": "jest --clearCache && jest --config ./jest.config.js --maxWorkers=50% --watch",
    "test:coverage": "jest --clearCache && jest --config ./jest.config.js --maxWorkers=50% --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.70.3",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-modalize": "^2.1.1",
    "react-native-paper": "^4.12.4",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.1.0",
    "@testing-library/jest-native": "^5.1.2",
    "@testing-library/react-native": "^11.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.0",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.2.1",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.2",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.2"
  }
}
